{
  "name": "backend",
  "version": "1.0.0",
  "description": "Template for back-end with Webpack",
  "main": "./dist/server.js",
  "scripts": {
    "compile": "tsc-watch",
    "start.dev": "nodemon  -w src  -e ts,tsx,env,graphql -w src -w config -w .env --exec 'ts-node ./src/server.ts'",
    "build.dev": "webpack --mode development",
    "build": "tsc",
    "start.bundle": "nodemon -e js,env -w . -w .env  -w dist dist/js/server.bundle.js",
    "start": "nodemon -e js,env -w . -w .env  -w dist dist/js/server.js",
    "tsc": "tsc",
    "lint": "eslint --ignore-pattern src --fix .",
    "watch.dev_old": "webpack --mode development --watch",
    "start.dev_old": "nodemon -e js,graphql,env -w . -w .env  -w dist -w server.js dist/js/server.js"
  },
