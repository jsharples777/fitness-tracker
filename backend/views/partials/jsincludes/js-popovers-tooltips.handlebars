<script>
    $(function () {
        $('[data-toggle="popover"]').popover()
    })
    $(function () {
        $('[data-toggle="tooltip"]').tooltip()
    })

    $(document).ready(function() {
        $(".toast").toast('show');
        $('.alert').alert();
        $('.popover-dismiss').popover({
            trigger: 'focus'
        })
    });
</script>